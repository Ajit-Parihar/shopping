<h1>UserAddresses#new</h1>
<p>Find me in app/views/user_addresses/new.html.erb</p>

<%= form_with model: @user_address do|f|%>

    <div>
        <%=f.label :country%>
        <%=f.text_field :country%>
    </div>

     <div>
        <%=f.label :state%>
        <%=f.text_field :state%>
    </div>

    <div>
        <%=f.label :dist%>
        <%=f.text_field :dist%>
    </div>

    <div>
        <%=f.label :block%>
        <%=f.text_field :block%>
    </div>


   <div>
        <%=f.label :town%>
        <%=f.text_field :town%>
    </div>


    <div>
        <%=f.label :gali_no%>
        <%=f.number_field :gali_no%>
    </div>

    <div>
        <%=f.label :house_no%>
        <%=f.number_field :house_no%>
    </div>

    <div>
        <%=f.submit%>
    </div>
<%end%>